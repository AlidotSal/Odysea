{
  "name": "odysea",
  "version": "0.5.4",
  "type": "module",
  "description": "A fast and lightweight SolidJS component library for building node-based diagrams and graphs.",
  "repository": {
    "url": "git+https://github.com/AlidotSal/odysea.git",
    "type": "git"
  },
  "homepage": "https://github.com/AlidotSal/odysea",
  "main": "dist/odysea.cjs",
  "module": "dist/odysea.js",
  "exports": {
    ".": {
      "import": "./dist/odysea.js",
      "require": "./dist/odysea.cjs"
    },
    "./package.json": "./package.json",
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "npx @biomejs/biome format --write ./src",
    "lint": "npx @biomejs/biome lint --write ./src",
    "build": "vite build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.5",
    "vite-plugin-dts": "^4.2.1",
    "@biomejs/biome": "^1.9.0",
    "postcss": "^8.4.45",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-solid-styled": "^0.11.1"
  },
  "dependencies": {
    "solid-js": "^1.8.22",
    "solid-styled": "^0.11.1"
  },
  "peerDependencies": {
    "solid-js": "^1.8.22"
  }
}
