{
  "name": "odysea",
  "version": "0.4.0",
  "description": "A fast and lightweight SolidJS component library for building interactive node-based diagrams",
  "repository": {
    "url": "https://github.com/AlidotSal/odysea.git",
    "type": "git"
  },
  "homepage": "https://github.com/AlidotSal/odysea",
  "main": "dist/odysea.umd.js",
  "module": "dist/odysea.mjs",
  "exports": {
    ".": {
      "import": "./dist/odysea.mjs",
      "require": "./dist/odysea.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "rome format src/ --write",
    "lint": "rome check src/ --apply",
    "build": "vite build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.9",
    "rome": "^10.0.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^1.3.0",
    "vite-plugin-solid": "^2.4.0",
    "vite-plugin-solid-styled": "^0.7.3"
  },
  "dependencies": {
    "solid-js": "^1.6.2",
    "solid-styled": "^0.7.3"
  },
  "peerDependencies": {
    "solid-js": "^1.6.2"
  }
}