    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "babel-plugin-solid-styled": "^0.4.1",
    "solid-js": "^1.4.3",
    "solid-styled": "^0.4.1"
  },
  "peerDependencies": {
    "solid-js": "^1.4.0"
  }
