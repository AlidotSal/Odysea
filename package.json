{
  "name": "solid-graph",
  "version": "0.1.10",
  "description": "A fast and lightweight SolidJS component library for building interactive node-based diagrams",
  "main": "dist/solid-graph.umd.cjs",
  "module": "dist/solid-graph.js",
  "exports": {
    ".": {
      "import": "./dist/solid-graph.js",
      "require": "./dist/solid-graph.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/style.css": "./dist/style.css"
